#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules/micro/dist/src/bin/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules/micro/dist/src/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules/micro/dist/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules/micro/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules/micro/dist/src/bin/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules/micro/dist/src/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules/micro/dist/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules/micro/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/micro@10.0.1/node_modules:/Users/myatshoonlei/Desktop/junioryear/project/pizza-ordering/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../micro/dist/src/bin/micro.js" "$@"
else
  exec node  "$basedir/../micro/dist/src/bin/micro.js" "$@"
fi
